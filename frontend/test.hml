  <script>
    function printPreviews(json) {
      var realEstates = json.immobilie;
      var html = '<table>';
      startLoading();

      for (realEstate of realEstates) {
        html += '<tr><td>' + immobrowse.preview(realEstate) + '</td></tr>';
      }

      document.getElementById('container').innerHTML = html + '</table>';
      stopLoading();
    }


